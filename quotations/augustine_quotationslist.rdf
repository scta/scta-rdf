   <rdf:Description rdf:about="http://scta.info/resource/quotation/Augustine_DeDoctrinaChristiana_1_2_2_b">
      <rdf:type rdf:resource="http://scta.info/resource/quotation"/>
      <dc:title>Ex quo intellegitur quid appellem signa: 
