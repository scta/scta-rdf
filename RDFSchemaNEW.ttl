######################################################
#Structure Elements.
#Generally all of these class will be predicated as rdf:type of parts of the texts
#which will all have the http: prefix of sctat: http://scta.info/text/ 
#followed by a commentary identifier such as /pg/ or /pp/ or /lm/ or /rfr/ 
######################################################

sctar:item 
		rdf:type rdfs:Class ; 
		rdfs:label "File Division" .    

sctar:text
	rdf:type rdfs:Class ;
	rdfs:label "Text Class" .

	sctar:structure  
		rdf:subClassOf sctar:text ;	
		rdfs:label "Structure Class" .  

		## item could become a property used to simply identify a file unit

		sctar:commentarius 
			rdfs:subClassOf sctar:structure ; 
	    	rdfs:label "Commentary Class" .

		sctar:librum 	
			rdfs:subClassOf sctar:structure ; 
			rdf:type rdfs:Class ;
		    rdfs:label "Book" .

		sctar:distinctio 
			rdfs:subClassOf sctar:structure ; 
			rdf:type rdfs:Class ;
		    rdfs:label "Distinction" .

	    sctar:articulus		
			rdfs:subClassOf sctar:structure ; 
			rdf:type rdfs:Class ;
		    rdfs:label "Article" .

	    sctar:lectio 		
			rdfs:subClassOf sctar:structure ; 
			rdf:type rdfs:Class ;
		    rdfs:label "Lecture" .

	    sctar:dubium 			
			rdfs:subClassOf sctar:structure ; 
			rdf:type rdfs:Class ;
		    rdfs:label "Dubium" .

	# after dubium -- divisions should be structured by the "element class" rather than the "structure class"

	sctar:element
		rdfs:subClassOf rdfs:text .

		sctar:quote
			rdf:type rdfs:Class ;
			rdfs:subClassOf sctar:element .

		sctar:name
			rdf:type rdfs:Class ;
			rdfs:subClassOf sctar:element .

		sctar:title
			rdf:type rdfs:Class ;
			rdfs:subClassOf sctar:element .

		sctar:reference	
			rdf:type rdfs:Class ;	
			rdfs:subClassOf sctar:element .

		sctar:paragraph				
			rdf:type rdfs:Class ;
			rdfs:subClassOf sctar:element .

