@prefix scta: <http://scta.info/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix dbpedia: <http://dbpedia.org/ontology/>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix dc: <http://purl.org/dc/elements/1.1/>.
@prefix scta-rel: <http://scta.info/relations/>.
@prefix scta-terms: <http://scta.info/terms/>.
@prefix role: <http://www.loc.gov/loc.terms/relators/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>    

scta:commentary 
	rdf:type rdfs:Class ;	
    rdfs:label "Commentary Class" .
    
scta:commentaryType
    rdf:type rdfs:Property ;
    rdfs:label "Commentary Type" ;
    rdfs:domain scta:commentary ;
    rdfs:range scta:commentary ;
    rdfs:dataType rdfs:Literal .

scta:structure  
	rdf:type rdfs:Class ;	
	rdfs:label "Structure Class" .  

	## item could become a property used to simply identify a file unit
	scta:item 
		rdfs:subClassOf scta:structure ; 
		rdf:type rdfs:Class ;
	    rdfs:label "Text Item" .    

	scta:book 	
		rdfs:subClassOf scta:structure ; 
		rdf:type rdfs:Class ;
	    rdfs:label "Book" .

	scta:distinction 
		rdfs:subClassOf scta:structure ; 
		rdf:type rdfs:Class ;
	    rdfs:label "Distinction" .

    scta:article 		
		rdfs:subClassOf scta:structure ; 
		rdf:type rdfs:Class ;
	    rdfs:label "Article" .

    scta:lecture 		
		rdfs:subClassOf scta:structure ; 
		rdf:type rdfs:Class ;
	    rdfs:label "lecture" .

scta:element
	rdf:type rdfs:Class .

	scta:quotation
		rdf:type rdfs:Class ;
		scta:subClassOf scta:element .

	scta:name
		rdf:type rdfs:Class ;
		scta:subClassOf scta:element .

	scta:title
		rdf:type rdfs:Class ;
		scta:subClassOf scta:element .

	scta:reference	
		rdf:type rdfs:Class ;	
		scta:subClassOf scta:element .

	scta:paragraph			
		rdf:type rdfs:Class ;
		scta:subClassOf scta:element .

scta:elementType
    rdf:type rdfs:Property ;
    rdfs:label "Element Type" ;
    rdfs:domain scta:element ;
    rdfs:dataType rdfs:Literal .

	scta:quoteType
	    rdf:subPropertyOf scta:elementType ;
	    rdfs:label "Quote Type" ;
	    rdfs:domain scta:quotation .

    scta:nameType		
	    rdf:subPropertyOf scta:elementType ;
	    rdfs:label "Name Type" ;
	    rdfs:domain scta:name .

    scta:titleType			
	    rdf:subPropertyOf scta:elementType ;
	    rdfs:label "Title Type" ;
	    rdfs:domain scta:title .

    scta:titleType				
	    rdf:subPropertyOf scta:elementType ;
	    rdfs:label "Reference Type" ;
	    rdfs:domain scta:reference .
	    
scta:quoteSource # this property should indicate the list source that he quote was retrieved from I.e. auctoritates, vulguate, custom, etc
    rdf:type rdfs:Property ;
    rdfs:label "Quote Source" ;
    rdfs:domain scta:quotation ;
    rdfs:dataType rdfs:Literal .
	
scta:pubInfo	
	rdf:type rdfs:Property ;
	rdfs:domain scta:structure ;
	rdfs:domain scta:item .

	rdf:type
		rdfs:subPropertyOf scta:pubInfo .

	scta-rel:status
		rdfs:subPropertyOf scta:pubInfo ;
		rdfs:dataType rdfs:Literal . # enumerated woudl be better: expects working draft, final draft, reviewed

	scta:creationDate
		rdfs:subPropertyOf scta:pubInfo ;
		rdfs:dataType rdfs:Literal . #is there a Date datatype??

	scta:mostRecentVersion
		rdfs:subPropertyOf scta:pubInfo ;
		rdfs:dataType rdfs:Literal . #is there a Date datatype??

	scta:collaborationStatus
		rdfs:subPropertyOf scta:pubInfo ;
		rdfs:dataType rdfs:Literal . #enumerated expects: closed versus open

	scta:itemEditor
		rdfs:subPropertyOf scta:pubInfo ;
		rdfs:dataType rdfs:Literal . #is there a Date datatype??

	scta:itemAuthor		
		rdfs:subPropertyOf scta:pubInfo ;
		rdfs:dataType rdfs:Literal . #is there a Date datatype??

	dc:title
		rdfs:subPropertyOf scta:pubInfo .

	role:AUT
		rdfs:subPropertyOf scta:pubInfo .

	role:EDT	
		rdfs:subPropertyOf scta:pubInfo .	

	scta-terms:citation
		rdfs:subPropertyOf scta:pubInfo .	

	scta-terms:fromBiblicalBook	
		rdfs:subPropertyOf scta:pubInfo .	
	
	scta-terms:fromBiblicalChapter		
		rdfs:subPropertyOf scta:pubInfo .	

	scta-terms:fromBiblicalVerse		
		rdfs:subPropertyOf scta:pubInfo .

	scta-terms:fromWork			
		rdfs:subPropertyOf scta:pubInfo .	

	scta-terms:fromWorkBook				
		rdfs:subPropertyOf scta:pubInfo .

	scta-terms:fromWorkChapter					
		rdfs:subPropertyOf scta:pubInfo .

	scta-terms:quotation			
		rdfs:subPropertyOf scta:pubInfo .
	
	scta-terms:quotetype			
		rdfs:subPropertyOf scta:pubInfo .	

scta:contentInfo
	rdf:type rdfs:Property ;
	rdfs:domain scta:item .
   	#rdfs:range scta:element .

	scta-rel:quotes
		rdfs:subPropertyOf scta:contentInfo ; 
		owl:inverseOf scta:isQuotedBy .

	scta-rel:quotedBy	
		rdfs:subPropertyOf scta:contentInfo ;
		owl:inverseOf scta:quotes . 

	scta-rel:mentions
		rdfs:subPropertyOf scta:contentInfo ;
		owl:inverseOf scta:isMentionedBy . 

	scta-rel:mentionedBy	
		rdfs:subPropertyOf scta:contentInfo ;
		owl:inverseOf scta:mentions .  

	scta-rel:references	
		rdfs:subPropertyOf scta:contentInfo ;
		owl:inverseOf scta:isReferencedBy . 

	scta-rel:referencedBy		
		rdfs:subPropertyOf scta:contentInfo ;
		owl:inverseOf scta:references . 


scta:linkingInfo
	rdf:type rdfs:Property .

	rdfs:seeAlso
		rdfs:subPropertyOf scta:linkingInfo .
	
	dcterms:hasPart
		rdfs:subPropertyOf scta:linkingInfo .

	dcterms:isPartOf	
		rdfs:subPropertyOf scta:linkingInfo .

	scta-rel:isTranscriptionOf	
		rdfs:subPropertyOf scta:linkingInfo .

	scta-rel:hasTranscription
		rdfs:subPropertyOf scta:linkingInfo .

   
